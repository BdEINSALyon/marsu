<h1><%= t('.register') %></h1>

<%= form_for @wei_registration do |f| %>
  <%= f.select :student_id, @students.map {|student| [student.name, student.id]}, {}, {class: 's2'} %>
  <%= f.submit t('.register'), class: 'btn btn-primary btn-block' %>
<% end %>

<%= link_to 'Back', wei_registrations_path, class: 'btn btn-block btn-secondary m-y-2'%>
